  "private": true,
  "scripts": {
    "start": "ionic serve --lab --port 8100",
    "clean": "ionic-app-scripts clean",
    "build": "ionic-app-scripts build --prod",
    "android": "SET NODE_ENV=android&& ionic cordova run android",
    "ios": "NODE_ENV=android ionic cordova run ios  --prod",
    "version-patch": "npm version patch --no-git-tag-version && node ./replace.build.js",
    "update-version": "node ./replace.build.js",
    "ionic:build": "ionic-app-scripts build",
    "ionic:serve": "ionic-app-scripts serve",
    "build-dev": "SET NODE_ENV=prod&&ionic-app-scripts build",
    "test": "karma start ./test-config/karma.conf.js  --coverage",
    "test-ci": "karma start ./test-config/karma.conf.js --single-run  --coverage",
    "e2e": "npm run e2e-update && npm run e2e-test",
    "e2e-test": "protractor ./test-config/protractor.conf.js",
    "e2e-update": "webdriver-manager update --standalone false --gecko false"
  },
  "config": {
    "ionic_webpack": "./config/webpack.config.js"
  },
